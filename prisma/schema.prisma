datasource db {
    provider     = "mongodb"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client-js"
}

model Journal {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    title     String
    content   String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    userId    String
}
